{
  "id": "http://some.site.somewhere/entry-schema#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "schema to test numeric restrictions",
  "definitions": {
    "age": {
      "type": "integer",
      "minimum": 0
    },
    "bestFriends": {
      "type": "integer",
      "maximum": 5
    },
    "two_to_three": {
      "type": "number",
      "maximum": 3.0,
      "maximumExclusive": true,
      "minimum": 2.0,
      "minimumExclusive": true
    },
    "less_than_three": {
      "type": "number",
      "maximum": 3.0,
      "maximumExclusive": true
    },
    "more_than_two": {
      "type": "number",
      "minimum": 2.0,
      "minimumExclusive": true
    },
    "multiples_of_three": {
      "multipleOf": 3
    },
    "allRestrictions": {
      "type": "number",
      "multipleOf": 3,
      "maximum": 16.0,
      "maximumExclusive": true,
      "minimum": 2.0,
      "minimumExclusive": true
    }
  }
}